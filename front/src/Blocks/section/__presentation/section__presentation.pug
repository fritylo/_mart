mixin section__presentation(sectionImage)
   .section__presentation.row.jcc&attributes(attributes)
      .section__presentation-back.w100.h100(style=`background-image: linear-gradient(to right, #fff 0%, #ffffff88 50%, #fff 100%), url(./attach/img/photo_${sectionImage});`)
         .section__presentation-front.w100.h100(style=`background-image: url(./attach/img/photo_${sectionImage});`)